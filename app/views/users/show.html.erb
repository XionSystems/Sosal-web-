<% content_for(:head) do %>
	<%= stylesheet_link_tag "gmaps4rails.css", "custom_mapShow.css" %>
	<script src="https://github.com/christianv/jquery-favicons/raw/master/jquery.favicons.min.js"></script> 
<% end %>
<% content_for(:scripts) do %>
	<%= javascript_include_tag "gmaps4rails.base.js", "gmaps4rails.googlemaps.js" %>
<% end %>
<% if current_user.following?(@user) || current_user?(@user) %>
	<%= render 'show_true' %>
<% else %>
	<%= render 'show_false' %>
<% end %>