<div class="user_show">
	<div class="user_nav">
		<% if current_user.profiles.blank?%>
			<%= link_to	default_photo, user_path(current_user) %>
		<% else %>
			<% @profile.each do |profile| %>
			<%= link_to (image_tag profile.avatar.url(:thumb)), user_path(current_user)  %>
			<% end %>
		<% end %>
		
		</div>
<div class="user_index_page">
<% @users.each do |user| %>
	<ul>
		<li>
			<%= link_to "#{user.username}", user %>
		</li>
	</ul>
<% end %>
</div>
</div>